{
    "name": "bryah/hermes",
    "description": "A fully featured messaging package for Laravel.",
    "keywords": ["laravel","messenger", "messaging", "chat", "mail"],
    "authors": [
        {
            "name": "Simon Schneider with slight modification by Andrew Christensen",
            "email": "info@Bryah.de"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "laravel/framework": "6.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/commands",
            "src/models",
            "src/classes",
            "src/facades",
            "src/config"
        ],
        "psr-0": {
            "Bryah\\Hermes\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
